{
  "name": "pm33-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "validate:css": "node scripts/validate-css.js",
    "validate:components": "node scripts/validate-components.js",
    "validate:config": "node scripts/validate-next-config.js",
    "validate:all": "npm run validate:css && npm run validate:components && npm run validate:config",
    "build:test": "NODE_ENV=production next build",
    "test:build": "node scripts/test-build.js",
    "quick-validate": "node scripts/quick-validate.js",
    "pre-deploy": "npm run validate:all && npm run build:test",
    "deploy:safe": "npm run pre-deploy && vercel --prod",
    "deploy:quick": "npm run quick-validate && vercel --prod",
    "clean-ports": "lsof -ti:3000 | xargs -r kill -9; lsof -ti:8000 | xargs -r kill -9; echo 'Ports 3000 and 8000 cleaned'",
    "dev-clean": "npm run clean-ports && npm run dev",
    "test:critical": "playwright test tests/pm33-ui-compliance.spec.ts tests/critical-path.spec.ts --project=chromium",
    "test:marketing": "playwright test tests/marketing/",
    "test:app": "playwright test tests/app/ tests/layout-validation.spec.ts --project=chromium",
    "test:ui-compliance": "playwright test tests/app/ui-compliance.spec.ts",
    "test:accessibility": "playwright test tests/pm33-ui-compliance.spec.ts --grep 'Accessibility'",
    "test:performance": "playwright test tests/performance/",
    "test:theme-contrast": "playwright test tests/theme-contrast-validation.spec.ts --project=chromium",
    "test:theme-contrast-comprehensive": "playwright test tests/theme-contrast-validation.spec.ts --grep 'All theme variants' --project=chromium",
    "test:theme-contrast-aaa": "playwright test tests/theme-contrast-validation.spec.ts --grep 'High contrast themes' --project=chromium",
    "test:contrast": "playwright test tests/contrast-compliance.spec.ts --project=chromium",
    "test:contrast-light": "playwright test tests/contrast-compliance.spec.ts --grep 'LIGHT Theme' --project=chromium",
    "test:contrast-dark": "playwright test tests/contrast-compliance.spec.ts --grep 'DARK Theme' --project=chromium",
    "test:contrast-gray": "playwright test tests/contrast-compliance.spec.ts --grep 'GRAY Theme' --project=chromium",
    "test:contrast-all": "npm run test:contrast-light && npm run test:contrast-dark && npm run test:contrast-gray",
    "test:visual-design": "playwright test tests/visual-design-validation.spec.ts --project=chromium",
    "test:layout": "playwright test tests/layout-validation.spec.ts --project=chromium",
    "test:ux-quality": "playwright test tests/ux-quality.spec.ts",
    "test:visual": "playwright test tests/visual-regression.spec.ts",
    "test:strategic-chat": "playwright test tests/step5-strategic-chat-component.spec.ts --project=chromium",
    "test:strategic-chat-vercel": "playwright test tests/strategic-chat-vercel.spec.ts --project=chromium",
    "test:step5": "npm run test:strategic-chat && npm run test:strategic-chat-vercel",
    "test:dashboard-health": "playwright test tests/dashboard-health-check.spec.ts --project=chromium --reporter=list",
    "test:three-column-layout": "playwright test tests/three-column-layout-validation.spec.ts --project=chromium --reporter=list",
    "test:global-css": "playwright test tests/global-css-validation.spec.ts --project=chromium --reporter=list",
    "test:ui-validation": "npm run test:dashboard-health && npm run test:three-column-layout && npm run test:global-css",
    "test:premium-experience": "npm run test:critical && npm run test:marketing && npm run test:accessibility && npm run test:theme-contrast-comprehensive && npm run test:contrast-all && npm run test:layout",
    "test:icons-buttons": "playwright test tests/icon-button-standardization.spec.ts tests/contrast-validation.spec.ts tests/theme-switching-stability.spec.ts tests/button-interaction-consistency.spec.ts --project=chromium --reporter=html",
    "test:icons-buttons-baseline": "playwright test tests/icon-button-standardization.spec.ts --update-snapshots --project=chromium",
    "test:contrast-wcag": "playwright test tests/contrast-validation.spec.ts --project=chromium --reporter=json",
    "test:theme-stability": "playwright test tests/theme-switching-stability.spec.ts --project=chromium --reporter=json",
    "test:button-interactions": "playwright test tests/button-interaction-consistency.spec.ts --project=chromium --reporter=json",
    "test:standardization-suite": "node scripts/run-standardization-tests.js",
    "test:all": "npm run test:marketing && npm run test:app && npm run test:ux-quality && npm run test:visual && npm run test:step5",
    "test:design-systems": "npm run test:marketing && npm run test:app",
    "test": "npm run test:all",
    "pre-commit": "npm run test:critical",
    "claude:verify": "node ../../scripts/verify-design-compliance.js",
    "validate:ui": "tsx scripts/validate-ui-consistency.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@mantine/charts": "^8.2.7",
    "@mantine/core": "^8.2.7",
    "@mantine/dates": "^8.2.7",
    "@mantine/dropzone": "^8.2.7",
    "@mantine/hooks": "^8.2.7",
    "@mantine/notifications": "^8.2.7",
    "@mantine/spotlight": "^8.2.7",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/supabase-js": "^2.54.0",
    "@tabler/icons-react": "^3.34.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.539.0",
    "next": "^15.5.0",
    "next-auth": "^4.24.11",
    "posthog-js": "^1.260.2",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.15.4",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
